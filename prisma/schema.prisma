generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id              String   @id @default(uuid())
  status          String
  gcsUrl          String
  surgeryName     String
  residentName    String?
  additionalContext String?
  result          String? // JSON string
  error           String?
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}